  "devDependencies": {
    "@rocketseat/eslint-config": "2.1.0",
    "@types/bcryptjs": "2.4.2",
    "@types/node": "20.4.8",
    "@types/supertest": "2.0.15",
    "@vitest/coverage-v8": "0.34.1",
    "@vitest/ui": "0.34.1",
    "eslint": "8.46.0",
    "npm-run-all": "4.1.5",
    "prisma": "5.1.1",
    "supertest": "6.3.3",
    "tsup": "7.2.0",
    "tsx": "3.12.7",
    "typescript": "5.1.6",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.33.0"
  },
  "dependencies": {
    "@fastify/jwt": "7.2.2",
    "@prisma/client": "5.1.1",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.9",
    "dotenv": "16.3.1",
    "fastify": "4.21.0",
    "zod": "3.21.4"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node build/src.js",
    "build": "tsup src --out-dir build",
    "test": "vitest run --dir src/useCases",
    "test:watch": "vitest --dir src/useCases",
    "test:create-prisma-env": "npm link ./vitest-environment-prisma",
    "test:install-prisma-env": "npm link vitest-environment-prisma",
    "pretest:e2e": "run-s test:create-prisma-env test:install-prisma-env",
    "test:e2e": "vitest run --dir src/http",
    "test:e2e:watch": "vitest --dir src/http",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
